<section id="add">
  <form action="/" method="post">
    <textarea name="content" placeholder='Add a title and the word "movie"&hellip;'></textarea>
    <input class="myButton" type="submit" value="ADD">
  </form>
</section>
<% @notes.each do |note| %>
  <article <%= 'class="complete"' if note.complete %>>
    <p>
      <%= note.content %>
      <span><a href="/<%= note.id %>/edit">Edit</a></span>
    </p>
    <p class="links"><a href="/<%= note.id %>/complete">&#8623;</a></p>
    <p class="meta">Created: <%= note.created_at %> in: </p>  
  </article>
<% end %>
